{
  "root": true,
  "env": {
      "browser": true,
      "node": true,
      "jest/globals": true
  },
  "extends": [
      "eslint:recommended",
      "plugin:vue/essential",
      "plugin:prettier/recommended",
      "prettier"
  ],
  "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module",
      "parser": "babel-eslint"
  },
  "plugins": [
    "prettier",
    "vue",
    "jest"
  ],
  "rules": {
    "prettier/prettier": "error"
  },
  "settings": {
    "jest": {
      "version": 27
    }
  }
}

